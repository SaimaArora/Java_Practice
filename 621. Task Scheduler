public int leastInterval(char[] tasks, int n) {
        int[] freq=new int[26];
        for(int i=0;i<tasks.length;i++){
            freq[tasks[i]-'A']++;
        }
        Arrays.sort(freq);
        int maxFreq=freq[25],freqCount=0;
        for(int i=25;i>=0;i--){
            if(maxFreq==freq[i]) freqCount++;
        }
        int timeMax=(maxFreq-1)*(n+1)+freqCount;
        int total=tasks.length;
        return Math.max(total,timeMax);
}
