public int maxMeetingsOrder(int[] start,int[] end){
        int n=start.length,result=1;
        int[][] meet=new int[n][3];
        for(int i=0;i<n;i++){
            meet[i]=new int[]{start[i],end[i],i};
        }

        Arrays.sort(meet,(a, b) -> a[1]-b[1]);
        int last=meet[0][1],i=1;
        while(i<n){
            int currLast=meet[i][1];
            int currFirst=meet[i][0];
            if(last<currFirst){
                last=currLast;
                result++;
            }
            i++;
        }
        return result;
    }
