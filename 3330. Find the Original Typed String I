class Solution {
    public int possibleStringCount(String word) {
        int n=word.length();
        int[] freq=new int[26];
        if(n<=0) return 1;
       int ans=1,i=0;
        while(i<n){
            while(i<n-1 && word.charAt(i)==word.charAt(i+1)){
                i++;
                ans++;
            }
            i++;
        }
        return ans;
    }
}
