class Solution {
    public int maxDepth(String s) {
        int n=s.length(),depth=0,ans=0;
        for(int i=0;i<n;i++){
            char ch=s.charAt(i);
            if(ch=='('){
                depth++;
                if(ans<depth) ans=depth;
            }
            else if(ch==')'){
                depth--;
            }
        }
        return ans;
    }
}
