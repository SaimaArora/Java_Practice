class Solution {
    private int square(int[] p) {
        return p[0] * p[0] + p[1] * p[1];
    }
    public int[][] kClosest(int[][] points, int k) {
        PriorityQueue<int[]> pq = new PriorityQueue<>((a, b) -> square(a) - square(b));
        for(int[] point : points) {
            pq.offer(point);
        }
        int[][] result = new int[k][2];
        int ind = 0;
        while(k-- > 0) {
            result[ind++] = pq.poll();
        }
        return result;
    }
}
