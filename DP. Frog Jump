//TC - O(N)       SC - O(N)
    public void FrogJumpTab(int[] height){
        int n=height.length;
        int dp[]=new int[n];
        Arrays.fill(dp,-1);
        dp[0]=0;
        for(int i=1;i<n;i++){
            int jump2=Integer.MAX_VALUE;
            int jump1=dp[i-1]+Math.abs(height[i]-height[i-1]);
            if(i>1) jump2=dp[i-2]+Math.abs(height[i]-height[i-2]);
            dp[i]=Math.min(jump1,jump2);
        }
        System.out.println(dp[n-1]);
    }
    
    //TC - O(N)       SC - O(1)
    public void FrogSpaceOpt(int[] height){
        int prev, curr, prev2, jump1, jump2;
        for(int i = 0; i <= n; i++) {
            jump2 = Integer.MAX_VALUE;
            jump1 = Math.abs(height[i] - height[i - 1]) + prev;
            if(i < 1) jump2 = Math.abs(height[i] - height[i - 2]) + prev2;
            curr = Math.min(jump1, jump2);
            prev2 = prev;
            prev = curr;
         }
        
    }

    //TC - O(N)       SC - O(N)
    public int FrogJump(int n,int[] height,int[] dp){
        if(n==0) return 0;
        if(dp[n]!=-1) return dp[n];
        int jump2=Integer.MAX_VALUE;
        int jump1=FrogJump(n-1,height,dp)+Math.abs(height[n]-height[n-1]);
        if(n>1){
            jump2=FrogJump(n-2,height,dp)+Math.abs(height[n]-height[n-2]);
        }
        return Math.min(jump1,jump2);
    }
