class Solution {
    public String longestCommonPrefix(String[] strs) {
        int len=strs[0].length(),n=strs.length,i=0;
        for( i=0;i<len;i++){
            char curr=strs[0].charAt(i);
            for(int j=1;j<n;j++){
                if(strs[j].length()<=i || curr!=strs[j].charAt(i)){
                    return strs[0].substring(0,i);
                }
            }
        }
        return strs[0].substring(0,i);
    }
}
