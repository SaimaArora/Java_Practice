class Solution {
    public int triangularSum(int[] nums) {
        int n = nums.length,len = 0;
        for(int i = 0; i < n; i++) {
            for(int num = n-1; num > len; num--) {
                nums[num] = (nums[num] + nums[num - 1]) % 10;
            }
            len++;
        }
        return nums[n-1];
    }
}
