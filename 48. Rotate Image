
class Solution {
    public void rotate(int[][] matrix) {
        int top=0,bot=matrix.length-1;
        int col=matrix[0].length;
        int row=matrix.length;
        while(top<bot){
            for(int i=0;i<col;i++){
                int temp=matrix[top][i];
                matrix[top][i]=matrix[bot][i];
                matrix[bot][i]=temp;
                
            }
            top++;bot--;
        }
        for(int i=0;i<row;i++){
            for(int j=i+1;j<col;j++){
                int temp=matrix[i][j];
                matrix[i][j]=matrix[j][i];
                matrix[j][i]=temp;
            }
        }
    }
}
