class Solution {

    private int isLeast(int[] potions, long num, long target) {
        int left = 0, right = potions.length - 1;
        int n = potions.length;
        int ans = -1;
        while(left <= right) {
            int mid = left + (right - left)/2;
            if(num * potions[mid] >= target) {
                ans = mid;
                right = mid - 1;
            }
            else {
                left = mid + 1;
            }
        }
        return ans;
    }

    public int[] successfulPairs(int[] spells, int[] potions, long success) {
        Arrays.sort(potions);
        int n = potions.length;
        int ans [] = new int[spells.length];
        for(int i = 0; i < spells.length; i++) {
            int res = isLeast(potions, (long)spells[i], success);
            if (res != -1) ans[i] = n - res;
        }
        return ans;
    }
}
