class Solution {
    public int maximumPossibleSize(int[] nums) {
        Stack<Integer> result=new Stack<>();
        int n=nums.length;
        for(int i=n-1;i>=0;i--){
             while(!result.isEmpty() && result.peek()<nums[i]){
                result.pop();
            }
            result.push(nums[i]);
        }
        return result.size();
    }
}
