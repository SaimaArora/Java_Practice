class Solution {
    public int matchPlayersAndTrainers(int[] play, int[] train) {
        Arrays.sort(play);
        Arrays.sort(train);
        int n=play.length,m=train.length,ans=0,j=0;
        for(int i=0;i<m && j<n;i++){
            if(play[j]<=train[i]){
                ans++;
                j++;
            }
        }
        return ans;
    }
}
