class Solution {
    public int recursive(String s,int i,int j){
        if(i>j) return 0;
        if(i==j) return 1;
        if(s.charAt(i)==s.charAt(j)){
            return 2+recursive(s,i+1,j-1);
        }
        else{
            return Math.max(recursive(s,i+1,j),recursive(s,i,j-1));
        }
    }
    public int memo(String s,int i,int j, int[][] dp){
        if(i>j) return 0;
        if(i==j) return 1;
        if(dp[i][j]!=-1) return dp[i][j];
        if(s.charAt(i)==s.charAt(j)){
            return dp[i][j]=2+memo(s,i+1,j-1,dp);
        }
        else{
            return dp[i][j]=Math.max(memo(s,i+1,j,dp),memo(s,i,j-1,dp));
        }
    }
    public int tabulation(String s){
        int n=s.length();
        int[][] dp=new int[n][n];
        for(int i=0;i<n;i++) dp[i][i]=1;
        for(int i=n-1;i>=0;i--){
            for(int j=i+1;j<n;j++){
                if(s.charAt(i)==s.charAt(j)){
                    dp[i][j]=2+dp[i+1][j-1];
                }
                else{
                    dp[i][j]=Math.max(dp[i+1][j],dp[i][j-1]);
                }
            }
        }
        return dp[0][n-1];
    }
    public int longestPalindromeSubseq(String s) {
        // int n=s.length();
        // int[][] dp=new int[n][n];
        // for(int[] rows: dp){
        //     Arrays.fill(rows,-1);
        // }
        return tabulation(s);
    }
}
