class Solution {
    public List<List<Integer>> subsets(int[] nums) {
        int n = nums.length;
        List<List<Integer>> subSets = new ArrayList<>();
        for (int b = 0; b < 1 << n; b++) {
            List<Integer> temp = new ArrayList<>();
            for (int i = 0; i < n; i++) {
                if ((b & (1 << i)) != 0) {
                    temp.add(nums[i]);
                }
            }
            subSets.add(temp);
        }
        return subSets;
    }
}
