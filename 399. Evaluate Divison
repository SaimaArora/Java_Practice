class Solution {
    Map<String, Map<String, Double>> graph = new HashMap<>();
    public double dfs(String start, String end, Set<String> visited, double ans){
        if(start.equals(end)) return ans;
        visited.add(start);
        Map<String, Double> neighbours = graph.get(start);
        for(String neighbour : neighbours.keySet()) {
            if(!visited.contains(neighbour)) {
                double res = dfs(neighbour, end, visited, ans * neighbours.get(neighbour));
                if(res != -1) return res;
            }
        }
        return -1.0;

    }
    public double[] calcEquation(List<List<String>> eq, double[] values, List<List<String>> queries) {
        
        double[] result = new double[queries.size()];
        for (int i = 0; i < eq.size(); i++) {
            String num = eq.get(i).get(0);
            String den = eq.get(i).get(1);
            double ans = values[i];
            graph.putIfAbsent(num, new HashMap<>());
            graph.putIfAbsent(den, new HashMap<>());
            graph.get(num).put(den, ans);
            graph.get(den).put(num, 1.0/ans);
        }
        for(int i = 0; i < queries.size(); i++) {
            String num = queries.get(i).get(0);
            String den = queries.get(i).get(1);
            Set<String> visited = new HashSet<>();
            if (!graph.containsKey(num) || !graph.containsKey(den)) {
                result[i] = -1.0;
            }
            else {
                result[i] = dfs(num, den, visited, 1.0);
            }

        }
        return result;
    }
}
