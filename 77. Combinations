class Solution {
    void backtrack(int start, List<Integer> comb, int k, int n, List<List<Integer>> res) {
        if (comb.size() == k){
            res.add(new ArrayList<>(comb));
            return;
        }
        for(int st = start; st <= n; st++) {
            comb.add(st);
            backtrack(st+1, comb, k, n, res);
            comb.remove(comb.size() - 1);
        }
    }
    public List<List<Integer>> combine(int n, int k) {
        List<List<Integer>> res = new ArrayList<>();
        backtrack(1, new ArrayList<>(), k, n, res);
        return res;
    }
}
