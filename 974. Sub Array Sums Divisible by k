class Solution {
    public int subarraysDivByK(int[] nums, int k) {
        HashMap<Integer, Integer> hs = new HashMap<>();
        int count = 0, prefix = 0;
        hs.put(0, 1);
        for(int num : nums) {
            prefix +=num;
            int remain = prefix%k;
            if(remain < 0) remain+=k;
            count+= hs.getOrDefault(remain, 0);
            hs.put(remain, hs.getOrDefault(remain, 0)+1);
        }
        return count;
    }
}
