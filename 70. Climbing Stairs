class Solution {

    public int tabulation(int n){
        int[] dp = new int[n+1];
        dp[0] = 1; dp[1] = 1;
        for(int i = 2; i <= n; i++) {
            dp[i] = dp[i - 1] + dp[i - 2];
        }
        return dp[n];
    }

    public int climbStairs(int n) {
        // int memo[] = new int[n+1];
        // Arrays.fill(memo, -1);
        // return recur(n, memo);
        return tabulation(n);
    }
    public int recur(int n, int[] memo) {
        if(n==0) return 1;
        if(n==1) return 1;
        if (memo[n]!= -1) return memo[n];
        return memo[n] = recur(n-1, memo)+recur(n-2, memo);
    }
}
